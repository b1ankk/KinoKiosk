<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1"
      prefHeight="600.0" prefWidth="400.0"
      spacing="10.0"
      fx:controller="com.msos.TicketMenuController"
>
    <children>
        <ListView fx:id="ticketListView" focusTraversable="false"
                  prefHeight="200.0"
                  VBox.vgrow="SOMETIMES"
        />
        <ButtonBar prefHeight="40.0">
            <buttons>
                <Button mnemonicParsing="false" text="BUY"/>
            </buttons>
        </ButtonBar>
    </children>
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
</VBox>
