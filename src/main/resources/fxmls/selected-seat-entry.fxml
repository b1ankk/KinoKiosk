<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<fx:root type="VBox" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <HBox>
         <children>
            <Label text="Ticket: ">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label text="1"
                   fx:id="ticketNumberValueLabel">
                <font>
                    <Font name="System Bold" size="18.0"/>
                </font>
            </Label>
         </children>
      </HBox>
      <GridPane>
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" />
          <ColumnConstraints hgrow="ALWAYS" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Row: " />
            <Label text="5" GridPane.columnIndex="1"
                   fx:id="rowNumberValueLabel"/>
            <Label text="Seat:" GridPane.rowIndex="1" />
            <Label text="13" GridPane.columnIndex="1" GridPane.rowIndex="1"
                   fx:id="seatNumberValueLabel"/>
         </children>
      </GridPane>
   </children>
   <padding>
      <Insets bottom="4.0" left="4.0" right="4.0" top="4.0" />
   </padding>
</fx:root>
